  1 int maxProfit(int* prices, int pricesSize) { 
  2  
  3     int maxProfit=0,tmpMaxProfit=0; 
  4  
  5     for(int i=0;i<pricesSize-1;i++) 
  6  
  7     { 
  8  
  9         tmpMaxProfit+=prices[i+1]-prices[i]; 
 10  
 11  
 12  
 13         if(tmpMaxProfit<0) 
 14  
 15         { 
 16  
 17             tmpMaxProfit=0; 
 18  
 19         } 
 20  
 21         if(tmpMaxProfit>maxProfit) 
 22  
 23         { 
 24  
 25             maxProfit=tmpMaxProfit; 
 26  
 27         } 
 28  
 29  
 30  
 31     } 
 32  
 33     return maxProfit; 
 34  
 35 }
